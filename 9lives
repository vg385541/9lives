<script>

    (function(jmsCommon) {

        let dataLayer = jmsCommon.dataLayer = jmsCommon.dataLayer || {};

        dataLayer.getCookie = function(name) {
            let cookie = {};

            document.cookie.split(';').forEach(function(item) {
                let [key,value] = item.split('=');
                cookie[key.trim()] = value;
            });
            return cookie[name];
        };

        dataLayer.setCookie = function(name, value, days = 1, path = '/') {
            let expires = new Date(Date.now() + (days * 24 * 60 * 60 * 1000)).toUTCString();
            document.cookie = name + '=' + encodeURIComponent(value) + '; expires=' + expires + '; path=' + path
        };

        dataLayer.deleteCookie = function(name, value, path = '/') {
            document.cookie = name + '=' + encodeURIComponent(value) + '; expires=Thu, 01 Jan 1970 00:00:00 UTC' + '; path=' + path
        };

        dataLayer.language = "en";

        dataLayer.getPageCategory = function(pageCategory) {

            let category = null;

            if( pageCategory && typeof pageCategory !== 'undefined' ){
                category = pageCategory;
            }

            return category;
        };

        dataLayer.getCategoryPageName = function(template) {
            let pageName = "(not available)";
                                                pageName = "a history of cats in the white house";
                                        return pageName;
        };

        dataLayer.categoryTemplateCheck = false;
        dataLayer.pageCategoryTemplates = ["acp", "acnp", "pcp", "pcnp", "rcp", "rcnp", "icp", "icnp", "remix"];
        dataLayer.checkForCategoryView = function() {
            let definedPageTemplate = false;
                            definedPageTemplate = 'adp';
                        if (definedPageTemplate) {
                dataLayer.pageCategoryTemplates.forEach(pageCategoryTemplate => {
                    if (pageCategoryTemplate === definedPageTemplate) {
                        dataLayer.categoryTemplateCheck = definedPageTemplate;
                    }
                });
                if (dataLayer.categoryTemplateCheck) {
                    window.dataLayer.push({
                        event: 'category view',
                        categoryInfo: {
                            category: dataLayer.getCategoryPageName(dataLayer.categoryTemplateCheck),
                            url: window.location.pathname,
                            type: dataLayer.categoryTemplateCheck
                        }
                    });
                }
            }
        };

        dataLayer.page = function() {

            window.dataLayer = window.dataLayer || [];

            let pageCategory = {
                siteSection: 'uncategorized',
                pageTemplate: 'uncategorized'
            };

                            pageCategory.siteSection = 'articles';
            
                            pageCategory.pageTemplate = 'adp';
            
            let params = new URLSearchParams(window.location.search);

            let pageInfo = {
                brand: "9\u0020lives",
                language: "en",
                //geoRegion: 'us',
                //countrySite: 'global-us',
                errorPage: 'false',
                pageName: document.title.toLowerCase(),
                queryString: params.toString() || '(not available)',
                pagePath: window.location.pathname
            };

            let source = dataLayer.getCookie('jmsSource');
            let medium = dataLayer.getCookie('jmsMedium');
            let campaign = dataLayer.getCookie('jmsCampaign');
            let statusCode = dataLayer.getCookie('jmsHttpStatusCode');

            if (statusCode){
                pageInfo.errorPage = statusCode;
                dataLayer.deleteCookie('jmsHttpStatusCode', '');
            }

            if(!source || source === '' || source === 'null') {
                if(params.has('utm_source')) {
                    source = params.get('utm_source').toLowerCase();
                    dataLayer.setCookie('jmsSource', source);
                    pageInfo.firstsource = dataLayer.getCookie('jmsSource');
                }
                else {
                    pageInfo.firstsource = '(not available)';
                }
            } else {
                pageInfo.firstsource = dataLayer.getCookie('jmsSource');
            }

            if(!medium || medium === '' || medium === 'null') {
                if(params.has('utm_medium')) {
                    medium = params.get('utm_medium').toLowerCase();
                    dataLayer.setCookie('jmsMedium', medium);
                    pageInfo.firstmedium = dataLayer.getCookie('jmsMedium');
                }
                else {
                    pageInfo.firstmedium = '(not available)';
                }
            } else {
                pageInfo.firstmedium = dataLayer.getCookie('jmsMedium');
            }

            if(!campaign || campaign === '' || campaign === 'null') {
                if(params.has('utm_campaign')) {
                    campaign = params.get('utm_campaign').toLowerCase();
                    dataLayer.setCookie('jmsCampaign', campaign);
                    pageInfo.firstcampaign = dataLayer.getCookie('jmsCampaign');
                }
                else {
                    pageInfo.firstcampaign = '(not available)';
                }
            } else {
                pageInfo.firstcampaign = dataLayer.getCookie('jmsCampaign');
            }

            let page = {
                pageInfo: pageInfo
            };

            let category = dataLayer.getPageCategory(pageCategory);

            if(category) {
                page.category = category;
            }

            let user = {
                jmsaID: dataLayer.getCookie('jmsa'),
                gaID: dataLayer.getCookie('_gid'),
                guid: dataLayer.getCookie('guid')
            };

            let jmshe = dataLayer.getCookie('jmshe');
            let jmss = dataLayer.getCookie('jmss');
            let jmsa = dataLayer.getCookie('jmsa');

            if(jmshe !== null && (typeof jmshe !== 'undefined') && jmshe.length > 1) {
                user.email_sha256 = jmshe;
            }

            privacyVariable ='-';
            if(jmss == null || (typeof jmss == 'undefined') && jmsa !== null) {
                privacyVariable = 'N';
            } else if (jmss == "true" && jmsa !== null) {
                privacyVariable = 'Y';
            } else {
                privacyVariable ='-';
            }
            user.iabPrivacyString = '1Y' + privacyVariable + '-';

            let blacklist = {
                "gtm.blacklist": [
                    "abtGeneric", "ta", "adm", "asp", "awct", "sp", "awc", "awj", "baut", "bb",
                    "bsa", "cts", "csm", "mpm", "mpr", "gclidw", "cegg", "crto", "html", "img",
                    "dstag", "flc", "fls", "m6d", "ela", "gcs", "ts", "hjtc", "infinity", "sca",
                    "k50Init", "ll", "bzi", "ljs", "ms", "mpm", "mpr", "messagemate", "mf", "ta",
                    "ndcr", "nudge", "okt", "omc", "messagemate", "pa", "pc", "pntr", "placedPixel",
                    "pijs", "qcm", "qpx", "fxm", "scjs", "scp", "sfc", "sfl", "sfr", "shareaholic",
                    "svw", "tdlc", "tdsc", "tc", "tdc", "twitter_website_tag", "uslt", "uspt", "vei",
                    "veip", "vdc", "xpsh", "yieldify"
                ]
            };

            var pageView = {
                event: 'dataLayerReady',
                page: page,
                user: user
            };

            if(jmss == "true") {
                pageView = Object.assign(pageView, blacklist);
            }

            window.dataLayer.push(pageView);

        };

        jmsCommon.dataLayer.page();
        jmsCommon.dataLayer.checkForCategoryView();


    })(window.jmsCommon = window.jmsCommon || {});

